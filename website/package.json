{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node src/bin/www.js",
    "stop": "npx kill-port 3000",
    "restart": "npm run stop && npm run start-dev",
    "build": "node stimulus_build.js && npm run sass",
    "sass": "sass templates/sass:public/css",
    "sass:watch": "sass --watch templates/sass:public/css",
    "start-dev": "concurrently --raw \"DEBUG=express:* nodemon src/bin/www.js\" \"node stimulus_build.js --watch\" \"npm run sass:watch\""
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "axios": "^1.7.8",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "di-ninja": "^1.13.2",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-session": "^1.18.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "packetjs-di": "^2.1.0",
    "twig": "~0.10.3"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "esbuild": "^0.24.0",
    "nodemon": "^3.1.7",
    "sass": "^1.81.0"
  }
}
